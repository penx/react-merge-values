{
  "name": "react-merge-values",
  "version": "0.0.1",
  "description": "A React component that merges the values and onChange events of several inputs",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "start": "npm run storybook",
    "build": "npm run build-storybook && npm run build:lib && npm run build:es",
    "build:lib": "rimraf lib && copyup \"./src/**/*.scss\" ./lib && babel src -d lib --source-maps",
    "build:es": "rimraf es && copyup \"./src/**/*.scss\" ./es && cross-env BABEL_ENV=es babel src -d es --source-maps",
    "test": "jest && eslint ./src",
    "eslint": "eslint ./src",
    "jest": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penx/base-react-component.git"
  },
  "keywords": [
    "Base",
    "React",
    "Component"
  ],
  "author": "Alasdair McLeay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/penx/base-react-component/issues"
  },
  "homepage": "https://github.com/penx/base-react-component#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.16.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {}
}
